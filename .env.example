# Google Cloud Configuration for Vertex AI
# Copy this file to .env and fill in your values

# Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=your-project-id-here

# Gemini model location (e.g., us-central1)
GEMINI_LOCATION=global

# Gemini model name (default model for general use)
GEMINI_MODEL=gemini-2.0-flash-lite-001

# Gemini model for summarization (cost-effective model)
GEMINI_SUMMARIZATION_MODEL=gemini-2.0-flash-lite-001

# Gemini model for grounding (full model with Google Search support)
GEMINI_GROUNDING_MODEL=gemini-2.0-flash-001

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Cache Configuration
CACHE_ENABLED=false
CACHE_TTL_SECONDS=60
